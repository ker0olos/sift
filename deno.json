{
  "lint": {
    "rules": {
      "tags": ["recommended"],
      "include": [
        "prefer-ascii",
        "camelcase",
        "eqeqeq",
        "no-const-assign",
        "no-eval",
        "no-throw-literal",
        "no-sparse-arrays",
        "explicit-function-return-type"
      ]
    }
  },
  "lock": false,
  "tasks": {
    "udd": "deno run -A https://deno.land/x/udd/main.ts mod.ts test.ts"
  },
  "fmt": {
    "useTabs": false,
    "singleQuote": true,
    "indentWidth": 2
  }
}
